@model RH_DELIRIO.Models.Familiares

@{
    ViewData["Title"] = "Detalhes Familiares";
}

<h1>@ViewData["Title"]</h1>

<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.nome_pai)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nome_pai)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.nome_mae)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nome_mae)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.nome_filho)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nome_filho)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.nasc_filho)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nasc_filho)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.funcionario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.funcionario.Nome)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-warning" asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a class="btn btn-info" asp-action="Index">Retornar</a>
</div>
